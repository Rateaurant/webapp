{
    "semi": true,
    "singleQuote": true,
    "arrowParens": "always",
    "useTabs": true,
    "bracketSpacing": true,
    "bracketSameLine": true,
    "plugins": [
        "prettier-plugin-svelte"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}